{
    "editor.minimap.enabled": false,
    "editor.lineNumbers": "relative",
    "workbench.colorTheme": "Default Dark+",

    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",

    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.colorDecorators": true,
    "tailwindCSS.includeLanguages": {
        "clojure": "html"
    },
    "tailwindCSS.experimental.classRegex": [
       ":class\\s*\"([^\"]*)\""
    ],

    "calva.keybindingsEnabled": true,
    "calva.autoOpenREPLWindow": false,
    "calva.autoOpenJackInTerminal": false,
    "calva.paredit.hijackVSCodeDefaults": false,
    "calva.referencesCodeLens.enabled": true,
    "calva.paredit.defaultKeyMap": "strict",
    "calva.paredit.strictPreventUnmatchedClosingBracket": true,
    "clover.experimental": true
}